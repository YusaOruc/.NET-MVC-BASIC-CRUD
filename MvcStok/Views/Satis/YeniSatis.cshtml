
@{
    ViewBag.Title = "YeniSatis";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<form class="form-group" method="post">
    <div class="form-group">
        <label for="exampleInputEmail1">Ürün ID</label>
        <input type="text" required="" name="URUN" class="form-control" placeholder="Müşteri Adını Giriniz">

    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Müşteri ID</label>
        <input type="text" required="" name="MUSTERI" class="form-control" placeholder="Müşteri Adını Giriniz">

    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Adet</label>
        <input type="text" required="" name="ADET" class="form-control" placeholder="Müşteri Adını Giriniz">

    </div>
    <div class="form-group">
        <label for="exampleInputEmail1">Fiyat</label>
        <input type="text" required="" name="FIYAT" class="form-control" placeholder="Müşteri Adını Giriniz">

    </div>
    

    <button type="submit" class="btn btn-primary">Submit</button>

</form>

